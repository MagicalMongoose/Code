    35	
    36	void printStudentInfo(const Student *studPtr)
    37	{
    38		cout << "Name: " << fixed << setw(20) << left << studPtr->name;
    39		cout << "Email: " << fixed << setw(20) << left << studPtr->email;
    40		cout << "ID: " << fixed << setw(20) << left << studPtr->id << endl;
    41	}
