  114	//Function Name: rockPaperScissor
  115	//Purpose: Compare the rock paper scissor result with two players
  116	//Input: person's option, and computer's option
  117	//Output: PERSON_WIN or COMPUTER_WIN, or Draw
  118	int rockPaperScissor(int person, int computer)
  119	{
  120	    //Please use ROCK, PAPER, SCISSOR here instead of 1, 2, 3
  121	
  122	    int result = 0;
  123	
  124	    if(person == ROCK && computer == PAPER)//(1) if person is ROCK and computer is PAPER
  125	        //(2) computer wins
  126	        result = COMPUTER_WIN;
  127	    else if(person == ROCK && computer == SCISSOR)//(3) if person is ROCK and computer is SCISSOR
  128	        //(4) person wins
  129	        result = PERSON_WIN;
  130	    else if(person == PAPER && computer == ROCK)//(5) if person is PAPER and computer is ROCK
  131	        //(6) person wins
  132	        result = PERSON_WIN;
  133	    else if(person == PAPER && computer == SCISSOR)//(7) if person is PAPER and computer is SCISSOR
  134	        // (8) computer wins;
  135	        result = COMPUTER_WIN;
  136	    else if(person == SCISSOR && computer == ROCK)//(9) if person is SCISSOR and computer is ROCK
  137	        //(10) computer wins
  138	        result = COMPUTER_WIN;
  139	    else if(person == SCISSOR && computer == PAPER)//(11) if person is SCISSOR and computer is PAPER
  140	       //(12) person wins
  141	        result = PERSON_WIN;
  142	    else
  143	       //(13) it is a draw
